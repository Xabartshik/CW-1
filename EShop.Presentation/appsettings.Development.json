{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "EShop.Application.Services.ProductService": "Debug",
        "EShop.Application.Services.ShopService": "Verbose"
      }
    }
  },
  "AppSettings": {
    "EnableDetailedLogging": true,
    "MaxProductsPerPage": 5
  },
  "ShopSettings": {
    "MaxShopsPerPage": 3
  }
}